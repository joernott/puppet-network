###
### File managed by Puppet
###
DEVICE=<%= @interface %>
NAME=<%= @interface %>
<% if @macaddress %>HWADDR=<%= @macaddress %>
<% end -%>
<% if@master %>MASTER=<%= @master %>
<% end -%>
<% if @OVS_BRIDGE %>
DEVICETYPE=ovs
TYPE=OVSPort
OVS_BRIDGE=<%= @ovs_bridge %>
<% else -%>
SLAVE=yes
TYPE=Ethernet
<% end -%>
<% if @ethtool_opts %>ETHTOOL_OPTS="<%= @ethtool_opts %>"
<% end -%>
NM_CONTROLLED=no
